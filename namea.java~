import java.util.Scanner;
import java.io.*;
class namea
{
    public static void main()
    {
        Scanner sc=new Scanner(System.in);    
        System.out.println("Hey enter your name below:");
        String name="";
        name=sc.nextLine(System.in());
        System.out.println("Hey, your name is "+name+" !");
        
    
    }
}

